<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="hs-0.1.0-delete-device-unreachable-trigger-event" author="yli">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="trigger_event"/>
            <tableExists tableName= "alert_definition" />
        </preConditions>
        <delete tableName="alert_definition">
            <where>uei like '%Unreachable'</where>
        </delete>
        <delete tableName="trigger_event">
            <where>trigger_event_type = 'DEVICE_UNREACHABLE'</where>
        </delete>
    </changeSet>
</databaseChangeLog>
